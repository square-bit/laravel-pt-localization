{
    "name": "squarebit/localization",
    "description": "Laravel PHP Framework - Portuguese Version",
    "license": "MIT",
    "authors": [
        {
            "name": "Squarebit",
            "email": "info@square-bit.com",
            "homepage": "https://square-bit.com/"
        }
    ],
    "homepage": "https://github.com/square-bit/localization",
    "keywords": ["laravel", "localization", "portuguese", "translations"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Squarebit\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Squarebit\\Localization\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Squarebit\\Localization\\LocalizationServiceProvider"
            ]
        }
    }
}
